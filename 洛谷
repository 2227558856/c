43.                               #include <iostream>
                                  using namespace std;
                                  int main()
                                  {
                                    int n,k=1,t=1;
                                    cin>>n;
                                    for (int i=1;i<=n*n;i++)
                                    {
                                      if(k<10)
                                      cout<<0<<k;
                                      else
                                      cout<<k;
                                      k++;
                                      t++;
                                      if(t>n) 
                                      {
                                        cout<<'\n';
                                        t=1;
                                      }
                                    }
                                    cout<<'\n';
                                    k=1;
                                    for (int i=1;i<=n;i++)
                                    {
                                      for (int j=1;j<=n-i;j++)
                                      {
                                        cout<<"  ";
                                      }
                                      while(k<=i*(i+1)/2)
                                      {
                                        if(k<10)
                                        cout<<0<<k;
                                        else
                                        cout<<k;
                                        k++;	
                                        }
                                      cout<<'\n';
                                      }
                                      return 0;
                                  }




44.                         #include <iostream>
                            #include <bits/stdc++.h>
                            using namespace std;
                            int main()
                            {
                              int maxi=0,mini=10,a[1000],n,s=0;
                              cin>>n;
                              for (int i=1;i<=n;i++)
                              {
                                cin>>a[i];
                                maxi=max(a[i],maxi);
                                mini=min(a[i],mini);
                                s=s+a[i];
                              }
                              s=s-maxi-mini;
                              double d=(double)s/(n-2);
                              printf ("%.2lf",d);
                              return 0;
                            }




45.                         #include <bits/stdc++.h>
                            using namespace std;
                            int main()
                            {
                              int n,x,k=1;
                              cin>>n;
                              while(1)
                              {
                                for (x=100;x>0;x--)
                                if ((x*7+k*21)*52==n)
                                {
                                  cout<<x<<'\n'<<k;
                                  return 0;
                                }
                              k++;
                              }
                              return 0;
                             } 



46.                     #include <bits/stdc++.h>
                        using namespace std;
                        int main()
                        {
                          int n,s=0,S=0,a[11],m;
                          for (int i=1;i<=12;i++)
                          {
                            cin>>a[i];
                            s+=300-a[i];
                            if(s<0)
                            {
                            cout<<'-'<<i;
                            return 0;
                            }
                            while (s>=100)
                            {
                              S=S+100;
                              s=s-100;
                            }
                           } 
                           m=S*6/5+s;
                           cout<<m;
                           return 0;
                         } 





47.                           #include <bits/stdc++.h>
                              using namespace std;
                              int main()
                              {
                                int n,a[n],j=0;
                                cin>>n;
                                for (int i=1;i<=n;i++)
                                {
                                  cin>>a[i];
                                  if (i==1)
                                  cout<<0<<" ";
                                  else
                                  {
                                    for (int k=1;k<i;k++)
                                    {
                                      if (a[i]>a[k])
                                      j++;	
                                    }
                                  cout<<j<<" ";
                                  j=0;
                                  }
                                }
                                return 0;
                              }




48.                           #include <bits/stdc++.h>
                              using namespace std;
                              int main()
                              {
                                int n,i=0,a[1000];
                                cin>>n;
                                int m=n;
                                while (n!=1)
                                {	
                                  if (n%2==0)
                                  n=n/2;
                                  else
                                  n=3*n+1;
                                  i++;
                                  a[i]=n;
                                }
                                for (int j=i;j>0;j--)
                                {
                                  cout<<a[j]<<" ";
                                }
                                cout<<m;
                                return 0;
                              }



49.                           #include <bits/stdc++.h>
                              using namespace std;
                              int main()
                              {
                                int a[100],k=0;
                                for (int i=1;;i++)
                                {
                                  cin>>a[i];
                                  if (a[i]==0)
                                  break;
                                  k++;
                                }
                                for (int j=k;j>0;j--)
                                {
                                  cout<<a[j]<<" ";
                                }
                                return 0;
                              }



50.                   #include <bits/stdc++.h>
                      using namespace std;
                      int z(int n)     //定义一个函数判断是否为质数,以返回值0,1来判断 
                      {
                        if (n==1)
                        return 0;
                        if (n==2)
                        return 1;
                        if (n%2==0)
                        return 0;
                        for (int i=2;i<n;i++)
                        {
                          if (n%i==0)
                          return 0;
                        }
                        return 1;
                       } 
                      int main()
                      {
                        int n,s=0,k=0;
                        cin>>n;
                        for (int i=2;s+i<=n;i++)       //判断质数口袋是否超载 
                        {
                          if(z(i)==1)
                          {
                            s+=i;
                            cout<<i<<'\n';
                            k++;
                          }
                        }
                        cout<<k;
                        return 0;
                      }
