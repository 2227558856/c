32.                       #include <iostream>
                          using namespace std;
                          int main()
                          {
                            long long n,s=0,m=0;
                            cin>>n;
                            for (int i=1;;i++)
                            {
                              s+=i*i;
                              m+=i;
                              if (m>n)
                              {
                              s-=i*(m-n);
                              m=n;
                              }
                              if (m==n)
                              break;
                            }
                            cout<<s;
                            return 0;
                          }




