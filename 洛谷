38.                   #include <iostream>
                      #include<bits/stdc++.h>
                      #include <cmath>
                      using namespace std;
                      double M(int n)
                      {
                        double s=1;
                        for (int i=1;i<=n;i++)
                        s=s*(1+sqrt(5))/2;
                        return s;
                       } 
                      double N(int n)
                      {
                        double s=1;
                        for (int i=1;i<=n;i++)
                        s=s*(1-sqrt(5))/2;
                        return s;
                       } 
                      int main()
                      {
                        int n;
                        cin>>n;
                          printf("%.2lf",(M(n)-N(n))/sqrt(5));
                        return 0;
                      }



39.             #include <bits/stdc++.h>              //数组方法
                using namespace std;
                int main()
                {
                  int a[1000],n;
                  cin>>n;
                  for (int i=1;i<=n;i++)
                   {
                    cin>>a[i];
                   }
                   sort(a+1,a+n+1);
                   cout<<a[n]-a[1];
                   return 0;
                 } 





40.                 #include <bits/stdc++.h>                  //赋值最大最小进行替换
                    using namespace std;
                    int main()
                    {
                      int maxi=-1,mini=1001,n,k;
                      cin>>n;
                      for (int i=1;i<=n;i++)
                      {
                        cin>>k;
                        maxi=max(maxi,k);
                        mini=min(mini,k);
                      }
                      cout<<maxi-mini;
                      return 0;
                    }



41.                           #include <bits/stdc++.h>
                              using namespace std;
                              int main()
                              {
                                int n,t,k,maxx=1,s=1;
                                cin>>n>>t;
                                for (int i=2;i<=n;i++)
                                {
                                  cin>>k;
                                  if (k-t==1)
                                  s++;
                                  else
                                  s=1;
                                  t=k;
                                  if (maxx<s)
                                  maxx=s;
                                }
                                cout<<maxx;
                                return 0;
                              }





