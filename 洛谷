                                    using namespace std;
                                    int main()
                                    {
                                        int a,b,c,s;
                                        cin>>a>>b;
                                        c=a*10+b;
                                        s=c/19;
                                        cout<<s;
                                        return 0;
                                    }




10.（计算多边形对角线的交点数）         #include <iostream>
                                      using namespace std;
                                      int main()
                                      {
                                          unsigned long long s,n;
                                          cin>>n;
                                          s=n * (n-1) / 2 * (n-2) / 3 * (n-3) / 4;
                                          cout<<s;
                                          return 0;
                                      }







11.                                     #include <iostream>
                                        using namespace std;
                                        int main()
                                        {
                                            int n,i;
                                            int a=0,b=0,c=0,d=0;
                                            cin>>n;
                                            if (n%2==0)
                                            i++;
                                            if (4<n&&n<=12)
                                            i++;
                                            if (i==2)
                                            a=1;
                                            if (i>0)
                                            b=1;
                                            if (i==1)
                                            c=1;
                                            if (i==0)
                                            d=1;
                                            cout<<a<<' '<<b<<' '<<c<<' '<<d;
                                            return 0;
                                        }




12.                                         #include <iostream>
                                            using namespace std;
                                            int main()
                                            {
                                                int n,i;
                                                cin>>n;
                                                if (n%400==0)
                                                i=1;
                                                else if(n%100==0)
                                                i=0;
                                                else if(n%4==0)
                                                i=1;
                                                cout<<i;
                                                return 0;
                                            }





13.                                       #include <iostream>
                                          using namespace std;
                                          int main ()
                                          {
                                              int n;
                                              cin>>n;
                                              if (n==0)
                                              cout<<"Today, I ate 0 apple.";
                                              if (n==1)
                                              cout<<"Today, I ate 1 apple.";
                                              if (n>1)
                                              cout<<"Today, I ate "<<n<<" apples.";
                                              return 0;
                                          }
