    （溢出）                                 using namespace std;
                                            int N(int n)
                                            {
                                              long long s=1;
                                              for (int i=1;i<=n;i++)
                                              s=s*i;
                                              return s;
                                            }
                                            int main()
                                            {
                                              long long s=0,n;
                                              cin>>n;
                                              for (int i=1;i<=n;i++)
                                              s=s+N(i);
