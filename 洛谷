1.                          #include <iostream>
                            #include <cstdio>
                            using namespace std;
                            long i,m,n,s,a[1000];
                            int main()
                            {
                                cin>>n;
                                for(i=1;i<=n;i++)
                                cin>>a[i];
                                for(i=1;i<=n-1;i++)
                                for(m=1;m<=n-i;m++)
                                if (a[m]>a[m+1])
                                {
                                swap (a[m],a[m+1]);
                                s++;
                                }
                                cout<<s;
                                return 0;
                            }



