13.对着课本硬抄下来的计算器             
                  #include<iostream>
                  using namespace std;
                  double cal(int a,char ch,int b)
                  {
                    double result;
                    if(a==0)
                      throw'x';
                    switch(ch)
                    {
                    case'+':
                      result=a+b;
                      break;
                    case'-':
                      result=a-b;
                      break;
                    case'*':
                      result=a*b;
                      break;
                    case'/':
                      if(b==0)
                        throw 0;
                      result=(double)a/b;
                      break;
                    default:
                      throw "输入错误，请重新输入！\n";
                    }
                    return result;
                  }
                  int main()
                  {
                    int a,b;
                    char ch;
                    while(1)
                    {
                      try
                      {
                        cout<<"请输入算式：";
                        cin>>a>>ch>>b;
                        cout<<a<<ch<<b<<"="<<cal(a,ch,b)<<endl;
                      }
                      catch(int)
                      {
                        cout<<"除数为0！\n";
                      }
                      catch(char*s)
                      {   cout<<s;
                      }
                      catch(...)
                      {   cout<<"谢谢使用！\n";
                        exit(0);
                      }
                    }
                    return 0;
                  }



