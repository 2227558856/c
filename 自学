			{
				int n;
				a t;
				n=sizeof(num)/sizeof(num[0]);
				for (int i=0;i<n;i++)
				{
					if(num[i].name>num[i+1].name)
					{
						t=num[i];
						num[i]=num[i+1];
						num[i+1]=t;
					}
				}
				for(int i=0;i<n;i++)
				{
					cout<<"姓名：" <<num[i].name<<endl;
					cout<<"成绩：" <<num[i].sorce<<endl;
