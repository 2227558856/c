			public:
				void abc(double x=5);
				double C(void);
				double S(void);
		};
		void what::abc(double x)
		{
			a=b=c=x;
		}
		double what::C(void)
		{
			return a+b+c;
		}
		double what::S(void)
		{
			double t=(a+b+c)/2;
			double s=sqrt(t*(t-a)*(t-b)*(t-c));
			return s;
		 } 
		int main()
