28.                     #include <iostream>
			using namespace std;
			int main()
			{
				int a,b;
				cin>>a>>b;
				cout<<"a="<<a<<'\t'<<"b="<<b<<endl;
				int *p1=&a,*p2=&b,*p;
				if(a>b)
				{
					p=p1;
					p1=p2;
					p2=p;
				}
				cout<<"从小到大输出"<<'\n'<<*p1<<'\t'<<*p2<<endl;
				cout<<"a="<<a<<'\t'<<"b="<<b;
			}



