              }




14.	            	#include <stdio.h>
                 int max(int x,int y)
                 {
                  int z;
                  if(x>y)z=x;
                  else z=y;
                  return(z);
                 }
                 int main()
                 {
                  int a,b,c;
                  scanf("%d,%d",&a,&b);
                  c=max (a,b);
                  printf("更大的是%d",c);
                  return 0;	
                  }




