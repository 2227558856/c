                      cin>>a>>b>>c;
                      cout<<"a="<<a<<'\n'<<"b="<<b<<'\n'<<"c="<<c<<endl;
                      if(a>b)t=b;
                      else t=a;
                      cout<<"最小的数是；"<<endl;
                      if(c>t)
                      cout<<t<<endl;
                      else
                      cout<<c<<endl;
                      return 0; 
                    }     
                    
                    
                    
                    
